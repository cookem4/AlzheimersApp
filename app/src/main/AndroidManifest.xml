<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mitch.alzheimersapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddMeds"
            android:label="@string/title_activity_add_meds"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MedList"
            android:label="@string/title_activity_med_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MedTaken"
            android:label="Medtaken"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AddContacts"
            android:label="@string/title_activity_add_contacts"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>